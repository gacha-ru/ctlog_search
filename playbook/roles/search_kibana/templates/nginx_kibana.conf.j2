server {
  listen                *:10080 ;

  server_name           localhost;
  access_log            /var/log/nginx/kibana.access.log;

  location /kibana/ {
    root  /usr/share/nginx/html;
    index  index.html  index.htm;
  }

  location / {
    proxy_pass http://127.0.0.1:9200;
    proxy_read_timeout 90;
  }
}
